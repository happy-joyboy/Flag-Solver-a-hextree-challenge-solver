<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="io.hextree.flagsolver"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="24"
        android:targetSdkVersion="35" />

    <queries>
        <package android:name="io.hextree.attacksurface" />
    </queries>

    <uses-permission android:name="android.permission.POST_NOTIFICATIONS" />

    <permission
        android:name="io.hextree.flagsolver.DYNAMIC_RECEIVER_NOT_EXPORTED_PERMISSION"
        android:protectionLevel="signature" />

    <uses-permission android:name="io.hextree.flagsolver.DYNAMIC_RECEIVER_NOT_EXPORTED_PERMISSION" />

    <application
        android:allowBackup="true"
        android:appComponentFactory="androidx.core.app.CoreComponentFactory"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:debuggable="true"
        android:extractNativeLibs="false"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher_angry_luffy"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_angry_luffy_round"
        android:supportsRtl="true"
        android:testOnly="true"
        android:theme="@style/Theme.FlagSolver" >
        <activity
            android:name="io.hextree.flagsolver.servicetest"
            android:exported="false" />

        <provider
            android:name="io.hextree.flagsolver.AttackProvider"
            android:authorities="ItsFadinG.github.io"
            android:enabled="true"
            android:exported="true" />

        <activity
            android:name="io.hextree.flagsolver.ImplicItintetHelper"
            android:exported="true" >
            <intent-filter>
                <action android:name="io.hextree.attacksurface.ATTACK_ME" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name="io.hextree.flagsolver.services"
            android:exported="false" />
        <activity
            android:name="io.hextree.flagsolver.deeplinks"
            android:exported="true" >
            <intent-filter>
                <action android:name="android.intent.action.VIEW" />

                <category android:name="android.intent.category.DEFAULT" />
                <category android:name="android.intent.category.BROWSABLE" />

                <data
                    android:host="token"
                    android:scheme="hex" />
            </intent-filter>
        </activity>
        <activity
            android:name="io.hextree.flagsolver.pendingHelper"
            android:exported="true" />
        <activity
            android:name="io.hextree.flagsolver.pendingIntent"
            android:exported="true" >
            <intent-filter>
                <action android:name="io.hextree.attacksurface.MUTATE_ME" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name="io.hextree.flagsolver.Providers"
            android:exported="true" >
            <intent-filter>
                <action android:name="io.hextree.FLAG33" />

                <category android:name="android.intent.category.DEFAULT" />

                <data
                    android:host="io.hextree.flag33_2"
                    android:scheme="content" />
            </intent-filter>
        </activity>
        <activity
            android:name="io.hextree.flagsolver.BroadcastReceivers"
            android:exported="false" />
        <activity
            android:name="io.hextree.flagsolver.Activity"
            android:exported="false" />
        <activity
            android:name="io.hextree.flagsolver.not_found"
            android:exported="false" />
        <activity
            android:name="io.hextree.flagsolver.implicitIntent"
            android:exported="true" />
        <activity
            android:name="io.hextree.flagsolver.AvtivityResult"
            android:exported="false" />
        <activity
            android:name="io.hextree.flagsolver.Hextree"
            android:exported="false" />
        <activity
            android:name="io.hextree.flagsolver.menu"
            android:exported="true" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <provider
            android:name="androidx.startup.InitializationProvider"
            android:authorities="io.hextree.flagsolver.androidx-startup"
            android:exported="false" >
            <meta-data
                android:name="androidx.emoji2.text.EmojiCompatInitializer"
                android:value="androidx.startup" />
            <meta-data
                android:name="androidx.lifecycle.ProcessLifecycleInitializer"
                android:value="androidx.startup" />
            <meta-data
                android:name="androidx.profileinstaller.ProfileInstallerInitializer"
                android:value="androidx.startup" />
        </provider>

        <receiver
            android:name="androidx.profileinstaller.ProfileInstallReceiver"
            android:directBootAware="false"
            android:enabled="true"
            android:exported="true"
            android:permission="android.permission.DUMP" >
            <intent-filter>
                <action android:name="androidx.profileinstaller.action.INSTALL_PROFILE" />
            </intent-filter>
            <intent-filter>
                <action android:name="androidx.profileinstaller.action.SKIP_FILE" />
            </intent-filter>
            <intent-filter>
                <action android:name="androidx.profileinstaller.action.SAVE_PROFILE" />
            </intent-filter>
            <intent-filter>
                <action android:name="androidx.profileinstaller.action.BENCHMARK_OPERATION" />
            </intent-filter>
        </receiver>
    </application>

</manifest>